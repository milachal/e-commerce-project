m;\n    display: inline-block;\n`\n\nconst OrdersContainer = styled.div`\n    margin-left: 3rem;\n`\n\nconst StyledBtn = styled(StyledButton)`\n    margin-left: 0;\n    width: 10rem;\n`\nexport default MyOrders","/Users/mila/Documents/dev/my-projects/online-store/src/hooks/index.js",["209"],"import { useState, useEffect } from 'react'\nimport authAPI from '../api/axios'\n\nexport const useAuth = (callback = () => {}, error) => {\n    const [isLoggedIn, setIsLoggedIn] = useState(false)\n    const [userStatus, setUserStatus] = useState('')\n\n    useEffect(() => {\n        const getAuthStatus = async () => {\n            try {\n                const response = await authAPI.get('/authorize-user')\n                if (response.data.status === 'admin' && response.status === 200) {\n                    setUserStatus('admin')\n                    setIsLoggedIn(true)\n                    return callback()\n                }\n                if (response.data.status === 'user' && response.status === 200) {\n                    setUserStatus('user')\n                    setIsLoggedIn(true)\n                    return callback()\n                }\n                \n            } catch(e) {\n                error ? error() : callback()\n                console.log('Not authenticated')\n            }\n        }\n        getAuthStatus()\n    }, [])\n    return [isLoggedIn, userStatus]\n}\n","/Users/mila/Documents/dev/my-projects/online-store/src/components/Cart/ProductQuantity.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/Cart/CartProductsData.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/Cart/CartFooter.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/Orders/OrderCompleted.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/Cart/CheckoutPage.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/Orders/OrderPage.js",[],"/Users/mila/Documents/dev/my-projects