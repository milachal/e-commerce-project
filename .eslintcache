account/login')\n    })\n    console.log(login)\n\n    return (\n        <>\n            <Navigation />\n            {loading ? <Spinner /> : (\n                <>\n                    <Header />\n                    {/* edit errors */}\n                    {/* <span>{error}</span>  */}\n                    <AccountMenu />\n                    <Switch>\n                        <Route exact path={path}>\n                            <PersonalInfo \n                                name={name}\n                                email={email}\n                                link={`${url}/edit`}\n                            />\n                        </Route>\n                        <Route path={`${path}/edit`}>\n                        <EditPersonalInfo \n                            url={`${url}/`}\n                            name={name}\n                            email={email}\n                            nameValue={name || ''}\n                            emailValue={email || ''}\n                            onChangeName={e => setName(e.target.value)}\n                            onChangeEmail={e => setEmail(e.target.value)}\n                        /> \n                        </Route>\n                        <Route path={`${path}/orders/history`}>\n                            <MyOrders />\n                        </Route>\n                    </Switch>\n                    <Footer /> \n                </>\n            )}   \n        </>\n    )\n}\n\nexport default AccountPage\n","/Users/mila/Documents/dev/my-projects/online-store/src/components/Profile/PersonalInfo.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/Profile/EditPersonalInfo.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/api/axios.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/ui/Spinner.js",[],"/Users/mila/Documents/dev/my-projects/online-store/src/components/Profile/AccountMenu.js",[],"/Users/mila/Documents/dev/my-projects/online-